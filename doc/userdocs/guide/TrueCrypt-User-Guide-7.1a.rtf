\par {\listtext\pard\plain\ltrpar \rtlch\fcs1 \af0 \ltrch\fcs0 \f3\insrsid11692034\charrsid2260929 \loch\af3\dbch\af13\hich\f3 \'b7\tab}\hich\af0\dbch\af13\loch\f0 
